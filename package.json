{
  "name": "parrotenv",
  "version": "1.0.0",
  "description": "ParrotEnv is a CLI framework that quickly sets up a development environment for your JavaScript-based project with standardized configurations for the language's essential development tools.",
  "keywords": [
    "JavaScript",
    "Development",
    "Environment",
    "CLI",
    "Assistant",
    "Boilerplate",
    "Bootstrap",
    "Standard",
    "Setup",
    "Tools",
    "Configuration"
  ],
  "author": "Rafael Fernandes de Lima",
  "maintainers": [
    "rafaelimaf"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafaelimaf/parrotenv.git"
  },
  "main": "./dist/index.js",
  "files": [
    "./templates/**/*",
    "./dist/**/*"
  ],
  "type": "module",
  "bin": {
    "parrotenv": "dist/index.js",
    "pe": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node ./dist/index.js",
    "dev": "ts-node ./src/index.ts"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.1.5",
    "nanospinner": "^1.1.0",
    "strip-ansi": "^7.0.1"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "parrotenv": "^0.5.3",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1"
  },
  "bugs": {
    "url": "https://github.com/rafaelimaf/parrotenv/issues"
  },
  "homepage": "https://github.com/rafaelimaf/parrotenv#readme"
}
