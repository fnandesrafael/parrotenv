{
  "name": "parrotenv",
  "version": "0.2.3",
  "description": "ParrotEnv is a TypeScript-based CLI tool that helps you set up a JavaScript/TypeScript or React project development environment.",
  "keywords": [
    "JavaScript",
    "Boilerplate",
    "Setup",
    "Development",
    "Configuration"
  ],
  "author": "Rafael Fernandes de Lima",
  "maintainers": [
    "rafaelimaf"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rafaelimaf/parrotenv"
  },
  "main": "./dist/index.js",
  "type": "module",
  "bin": {
    "parrotenv": "./dist/index.js",
    "pe": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node ./dist/index.js",
    "dev": "ts-node ./src/index.ts"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "inquirer": "^9.1.5"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1"
  }
}
